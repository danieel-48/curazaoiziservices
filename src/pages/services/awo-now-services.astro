---
import Layout from '@/layouts/PageLayout.astro';
import BannerContactServices from '@/components/viewmore/BannerViewMore.astro';
import DocumentContactServices from '@/components/viewmore/DocumentViewMore.astro';

const metadata = {
  title: 'AWO (Now) Services | Curazao izi services',
  h1: 'AWO (Now) Services',
  text: 'At Curaçao IZI Services , we specialize in document pick-up and delivery, ensuring all your important papers are handled with care and precision. Our AWO (NOW) Services include the collection and delivery of both standard and certified documents. Whether it’s a single document or an entire package, you can trust us to manage it efficiently and securely.',
  text_1: 'Let us take the stress out of document management so you can focus on what truly matters. Contact us today for reliable and hassle-free services!',
  img: '/services/ImagesServiceFormContact.webp',
  ignoreTitleTemplate: true,
  actualServices: 'vehicle-management'
};
---

<Layout metadata={metadata}>
	<BannerContactServices></BannerContactServices>
	<DocumentContactServices metadata={metadata}></DocumentContactServices>
</Layout>


