---
import Layout from '@/layouts/PageLayout.astro';
import BannerContactServices from '@/components/viewmore/BannerViewMore.astro';
import DocumentContactServices from '@/components/viewmore/DocumentViewMore.astro';

const metadata = {
  title: 'AWO (Now) Services | Curazao izi services',
  h1: 'AWO (Now) Services',
  text: 'At Curaçao IZI Services, we specialize in document pick-up and delivery, ensuring all your important papers are handled with care and precision.<br> Our AWO (NOW) Service is designed to prioritize urgent requests — when a client calls, this service goes to the top of our list. It includes the collection and delivery of both standard and certified documents. Whether it’s a single document or an entire package, you can count on us to handle it quickly, securely, and professionally.',
  text_1: 'Let us take the stress out of document management so you can focus on what truly matters. Contact us today for fast, reliable, and hassle-free service!',
  img: '/images/services/Carrusel home CIS Image 3.jpg',
  ignoreTitleTemplate: true,
  actualServices: 'awo-now-services'
};
---

<Layout metadata={metadata}>
	<BannerContactServices></BannerContactServices>
	<DocumentContactServices metadata={metadata}></DocumentContactServices>
</Layout>


